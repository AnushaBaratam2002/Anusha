import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    String str1=sc.nextLine();
	    int len=str1.length();
	    int count=0;
	    for(int i=0;i<len;i++)
	    {
	        if(str1.charAt(i)== ' ' && str1.charAt(i-1)!=' ')
	        count++;
	    }
		System.out.println(count+1);
	}
}

