import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    String str1=sc.nextLine();
	      int digits = 0;
	      for (int i = 0; i < str1.length(); i++) {
            if (str1.charAt(i) >= 48 && str1.charAt(i) <= 57)
                digits++;
	      }
	      System.out.println("Total number of Digits = "
                           + digits);
	      
		
	}
}

    
